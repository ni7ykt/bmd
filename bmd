local StarterGui = game:GetService("StarterGui")
local Players = game:GetService("Players")
local Whitelisted = {
    ["HYHuiBai"] = true,
    ["xjdmed"] = true,
    ["XXXALASKKM"] = true,
    ["limyizhesabi"] = true,
    ["gary43829"] = true,
    ["KimberlyPorterdzxy78"] = true,
    ["nuiaKZcFs09y6"] = true,
 }

local function IsWhitelisted(player)
    return Whitelisted[player.Name] or false
end

local localPlayer = Players.LocalPlayer

local isLocalPlayerWhitelisted = IsWhitelisted(localPlayer)

if isLocalPlayerWhitelisted then
    StarterGui:SetCore("SendNotification", {
        Title = "白名單認證",
        Text = "玩家:"..localPlayer.Name.."，检测完毕白名单玩家，稍后会加载脚本",
        Duration = 7,
    })
loadstring(game:HttpGet("https://raw.githubusercontent.com/ni7ykt/LX_HUB/refs/heads/main/Lingxiao-hub"))()
else
    localPlayer:Kick("请联系作者添加白名单")
end
